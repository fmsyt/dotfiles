alias ls='command ls --human-readable --group-directories-first --color=auto -I NTUSER.DAT\* -I ntuser.dat\*'

alias ll='ls -lF'
alias la='ls -AlF'
alias l='ls -CF'

alias ga='git add'
alias gc='git commit'
alias gd='git diff'
alias gf='git fetch'
alias gm='git merge'
alias gs='git status'
alias gco='git checkout'
alias gcp='git cherry-pick'

