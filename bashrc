alias ls='command ls --human-readable --group-directories-first --color=auto --ignore={NTUSER*,ntuser*}'

alias ll='ls -lF'
alias la='ls -AlF'
alias l='ls -CF'

alias ga='git add'
alias gc='git commit'
alias gd='git diff'
alias gf='git fetch'
alias gp='git pull'
alias gs='git status'
alias gco='git checkout'

